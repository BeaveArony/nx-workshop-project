    },
    "directive": {
      "spec": false
    },
    "module": {
      "spec": false
    },
    "pipe": {
      "spec": false
    },
    "service": {
      "spec": false
    },
    "schematics": {
      "collection": "@nrwl/schematics",
      "newProject": [
        "app",
        "lib"
      ],
      "postGenerate": "npm run format"
    }
